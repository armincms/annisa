{
    "name": "armincms/annisa",
    "license": "MIT",
    "keywords": ["annisa", "laravel", "form"],
    "description" : "A laravel form builder/manager based on laravelcollective package; inspired of joomla form builder.",
    "authors": [
        {
            "name": "Esmaiel Zareh",
            "email": "zarehesmaiel@gmail.com"
        }
    ], 
    "require": {
        "laravelcollective/html": "6.*"
    },
    "autoload": {
        "psr-4": {
            "Annisa\\Form\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Annisa\\Form\\AnnisaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
